<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EssayServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.EssayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DailyServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.DailyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommentServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.CommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MsgServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.MsgServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MsgReplyServlet</servlet-name>
        <servlet-class>main.sun.bk.server.servlet.MsgReplyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EssayServlet</servlet-name>
        <url-pattern>/essay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DailyServlet</servlet-name>
        <url-pattern>/daily</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommentServlet</servlet-name>
        <url-pattern>/comment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MsgServlet</servlet-name>
        <url-pattern>/msg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MsgReplyServlet</servlet-name>
        <url-pattern>/msgReply</url-pattern>
    </servlet-mapping>

    
</web-app>